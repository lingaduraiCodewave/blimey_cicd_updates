{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "blimey-desktop",
  "version": "0.1.4",
  "identifier": "com.blimey.desktop",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "blimey-desktop",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "devtools": true,
        "userAgent": null
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://tauriupdatesdemo123.blob.core.windows.net/updates/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDlCQzI2RUU4M0YzQjY1MEUKUldRT1pUcy82RzdDbTdrcDVTejdac0k5U0ZCMVNrVk1XQmU4T2VhZHR6NlJaYmFCTGNCTUI2RUwK"
    }
  }
}
